{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/banner.svg",
      "dest": "/public/banner.svg"
    }
  ],
  "headers": [
    {
      "source": "*.svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    }
  ]
}
